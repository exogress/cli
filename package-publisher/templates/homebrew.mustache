class Exogress < Formula
  desc "Exogress command-line client"
  homepage "https://www.exogress.com/"
  version "{{ VERSION }}"
  url "{{ MACOS_URL }}"
  sha256 "{{ MACOS_SHA256 }}"
  license "Apache-2.0"

  depends_on "rust" => :build

  def install
    system "cargo", "install", *std_cargo_args
  end
end
